{
  "name": "web-client",
  "version": "0.1.0",
  "author": "Kunal Mandalia <kunal.v.mandalia@gmail.com> (http://kunalmandalia.com/)",
  "private": true,
  "dependencies": {
    "antd": "3.10.1",
    "apollo-boost": "0.1.16",
    "graphql": "14.0.2",
    "react": "^16.5.2",
    "react-apollo": "2.2.4",
    "react-dom": "^16.5.2",
    "react-scripts-ts": "3.1.0",
    "styled-components": "4.0.2"
  },
  "scripts": {
    "start": "react-app-rewired start --scripts-version react-scripts-ts",
    "build": "react-app-rewired build --scripts-version react-scripts-ts",
    "test": "react-app-rewired test --env=jsdom --scripts-version react-scripts-ts",
    "ci-test": "react-app-rewired test --env=jsdom --coverage --scripts-version react-scripts-ts",
    "deploy-heroku": "cd ../ && git subtree push --prefix web-client heroku-web-client master || true"
  },
  "devDependencies": {
    "@types/antd": "1.0.0",
    "@types/enzyme": "3.1.14",
    "@types/graphql": "14.0.3",
    "@types/jest": "23.3.5",
    "@types/node": "10.11.7",
    "@types/react": "16.4.16",
    "@types/react-dom": "16.0.9",
    "@types/react-test-renderer": "16.0.3",
    "@types/styled-components": "4.0.1",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.6.0",
    "enzyme-to-json": "3.3.4",
    "jest-styled-components": "6.2.2",
    "react-app-rewire-less": "2.1.3",
    "react-app-rewired": "1.6.2",
    "react-test-renderer": "16.6.0",
    "ts-import-plugin": "1.5.5",
    "typescript": "3.1.3"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
